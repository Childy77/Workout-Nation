<h1>Create My Workout</h1>
<form>
    <div class="mb-3">
        <label for="taskName" class="form-label">Workout Name</label>
        <input type="email" class="form-control" id="taskName">
    </div>

    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="0">
        <label class="form-check-label" for="inlineRadio1">Low</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="1">
        <label class="form-check-label" for="inlineRadio2">Moderate</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio3" value="2">
        <label class="form-check-label" for="inlineRadio3">High</label>
    </div>
    <div class="mb-3">
        <label for="Time" class="form-label">Time</label>
        <input type="text" class="form-control" id="time">
    </div>
    <div class="d-grid gap-2">
        <button class="btn btn-primary" type="submit" id="submitBtn">Submit</button>
    </div>

</form>

<script src="/js/submit-task.js"></script>